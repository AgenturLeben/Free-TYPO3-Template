config {
  # Seitentyp
  #xhtml_cleaning = all
  disablePrefixComment = 1
  #index_enable = 1
  #renderCharset = utf-8
  #metaCharset = utf-8
  #additionalHeaders = Content-Type:text/html;charset=utf-8
  #doctype = html5
  
  # URL-Handling
  simulateStaticDocuments = 0
  tx_realurl_enable = 1
  #prefixLocalAnchors = all
  
  # Sprache
  linkVars = L
  sys_language_mode = content_fallback
  
  # Sprache: deutsch
  baseURL = {$baseURL}
  #sys_language_uid = 0
  language = de
  locale_all = de_DE
  htmlTag_langKey = de-DE
  
  # Bilder
  noScaleUp = 1
  
  # Spam
  spamProtectEmailAddresses = 1
  spamProtectEmailAddresses_atSubst = (at)
  
  # Frontend Edit
  #admPanel = 1
}

[globalVar = TSFE : beUserLogin > 0]
config.no_cache = 1
[global]

page = PAGE
page {
  config {
    htmlTag_stdWrap {
      cObject = TEXT
      cObject.value (
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
      )
    }
  }
  
  htmlTag_setParams = class="no-js" 
  
  meta {
    #robots = noindex,nofollow
    description {
      data = TSFE:page|description
    }
    keywords {
      data = TSFE:page|keywords
      keywords = 1
    }
    viewport = width=device-width,initial-scale=1.0
    X-UA-Compatible = IE=edge,chrome=1
    X-UA-Compatible.httpEquivalent = 1
  }
  
  includeJS {
    file1 = fileadmin/templates/Public/JS/modernizr-2.6.2-respond-1.1.0.min.js
  }
  
  includeCSS {
    file1 = fileadmin/templates/Public/CSS/normalize.min.css
    file2 = fileadmin/templates/Public/CSS/fonts.css
    file3 = fileadmin/templates/Public/CSS/main.css
  }
  
  bodyTagCObject = TEXT
  bodyTagCObject.field = uid
  bodyTagCObject.wrap = <body lang="de" id="body|" class="col3-layout">
  
  10 = TEMPLATE
  10 {
   
    template = FILE
    template.file = fileadmin/templates/Private/HTML/col3.html
    
    workOnSubpart = LAYOUT
    
    subparts {
      
      CONTENT_M = CONTENT
      CONTENT_M {
        table = tt_content
        select {
          where = colPos=0
          orderBy = sorting
          languageField = sys_language_uid
        }
      }
      
      CONTENT_L = CONTENT
      CONTENT_L {
        table = tt_content
        slide = -1
        select {
          where = colPos=1
          orderBy = sorting
          languageField = sys_language_uid
        }
      }
      
      CONTENT_R = CONTENT
      CONTENT_R {
        table = tt_content
        slide = -1
        select {
          where = colPos=2
          orderBy = sorting
          languageField = sys_language_uid
        }
      }
      
      HEADER = CONTENT
      HEADER {
        table = tt_content
        slide = -1
        select {
          where = colPos=3
          orderBy = sorting
          languageField = sys_language_uid
        }
      }
      
      #HEADER = FILES
      #HEADER {
      #  references {
      #    table = pages
      #    uid.data = page:uid
      #    fieldName = media
      #  }
      #  stdWrap.wrap = <div class="flexslider"><ul class="slides">|</ul></div>
      #  renderObj = COA
      #  renderObj {
      #    10 = IMAGE
      #    10 {
      #      altText.data = file:current:title
      #      titleText.data = file:current:description
      #      
      #      wrap = <li>|</li>
      #      file {
      #        import.data = file:current:originalUid // file:current:uid
      #        width = 960
      #        height = 315
      #      }
      #    }
      #  }
      #}
      
      FOOTER = CONTENT
      FOOTER {
        table = tt_content
        slide = -1
        select {
          where = colPos=4
          orderBy = sorting
          languageField = sys_language_uid
        }
      }
      
      LOGO = IMAGE
      LOGO {
        file = fileadmin/templates/Public/IMG/{$LOGO}
        file {
          width = 130
          height = 64
        }
        altText = {$TITLE}
        titleText = {$SLOGAN}
        stdWrap.typolink.parameter = {$WEBSITE_ID}
      }
      
      GOOGLE_TAG_MANAGER = TEXT
      GOOGLE_TAG_MANAGER.value = {$GOOGLE_TAG_MANAGER}
      
    }
  }
  3000 = TEXT
  3000.value (
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js"></script>
  <script>window.jQuery || document.write('<script src="fileadmin/templates/Public/JS/jquery-1.10.1.min.js"><\/script>')</script>
  )
  #includeJSFooterlibs {
  #  file1 = fileadmin/templates/Public/JS/jquery.flexslider-min.js
  #}
  includeJSFooter {
    file1 = fileadmin/templates/Public/JS/main.js
  }
  
}

# LAYOUTS
[globalVar=TSFE:page|layout=1]
page.10.template.file = fileadmin/templates/Private/HTML/col2_left.html
page.bodyTagCObject.wrap = <body lang="de" id="body|" class="col2l-layout">
[globalVar=TSFE:page|layout=2]
page.10.template.file = fileadmin/templates/Private/HTML/col2_right.html
page.bodyTagCObject.wrap = <body lang="de" id="body|" class="col2r-layout">
[globalVar=TSFE:page|layout=3]
page.10.template.file = fileadmin/templates/Private/HTML/col1.html
page.bodyTagCObject.wrap = <body lang="de" id="body|" class="col1-layout">
[global]

# BROWSER
[browser = msie] && [version < 7] 
  config.htmlTag_setParams = class="no-js ie6" 
[browser = msie] && [version = 7] 
  config.htmlTag_setParams = class="no-js ie7" 
[browser = msie] && [version = 8] 
  config.htmlTag_setParams = class="no-js ie8" 
[global]


